# File 1: konversi.py
def c_to_f(c):
    return (c * 9/5) + 32

def f_to_c(f):
    return (f - 32) * 5/9


# File 2: main.py
import konversi

c = float(input("Masukkan suhu Celcius: "))
print("Suhu dalam Fahrenheit =", konversi.c_to_f(c))

f = float(input("Masukkan suhu Fahrenheit: "))
print("Suhu dalam Celcius =", konversi.f_to_c(f))
